<%= form_for(@shop) do |f| %>
 <h4>住所</h4>
 <%= f.text_field :address, placeholder: "住所" %>
 <%= f.submit "登録する" %>
<% end %>

<table>
	<% @shops.each do |shop|%>
	<tr>
		<th>住所</th>
		<th></th>
	</tr>
	<tr>
		<td><%= shop.address%></td>
		<td><%= link_to "show", shop_path(shop.id)%></td>
	</tr>
	<% end %>
</table>